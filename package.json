{
  "name": "@pnavarrc/hit",
  "description": "Git for Humans",
  "version": "0.0.1",
  "author": "Pablo Navarro Castillo @pnavarrc",
  "bin": {
    "hit": "./bin/run"
  },
  "bugs": "https://github.com/pnavarrc/hit/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "eslint-config-prettier": "^8.3.0",
    "globby": "^10",
    "husky": "^7.0.0",
    "prettier": "2.4.1",
    "pretty-quick": "^3.1.1",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/pnavarrc/hit",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "hit",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "pnavarrc/hit",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "prepare": "husky install"
  },
  "types": "lib/index.d.ts"
}
